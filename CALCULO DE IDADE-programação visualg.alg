algoritmo "calculo de idade"

var

nome:caractere
nascimento:inteiro
ano:inteiro


inicio


escreval("olá, qual seu nome?")
leia(nome)
escreval("qual seu ano de nascimento?")
leia(nascimento)
escreval("qual o ano atual?")
leia(ano)
escreva("você tem ",ano-nascimento," anos de idade")




fimalgoritmo